{
  "compilerOptions": {
    "baseUrl": "./",
    "target": "es6",
    "checkJs": true,
    "noEmit": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "jsx": "react",
    "module": "commonjs",
    "paths": {
      "@src/*": [
        "src/*"
      ],
    }
  },
  "exclude": [
    "node_modules"
  ]
}